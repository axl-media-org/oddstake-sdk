{
    "name": "axl-media-org/oddstake-sdk",
    "description": "SDK for the Oddstake service.",
    "keywords": ["laravel", "php"],
    "license": "Apache-2.0",
    "homepage": "https://github.com/axl-media-org/oddstake-sdk",
    "authors": [
        {
            "name": "AXL Media",
            "homepage": "https://axl.media",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0"
    },
    "autoload": {
        "psr-4": {
            "AxlMedia\\OddstakeSdk\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AxlMedia\\OddstakeSdk\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "require-dev": {
        "laravel/legacy-factories": "^1.1",
        "mockery/mockery": "^1.4",
        "orchestra/testbench": "^5.0|^6.0",
        "orchestra/database": "^5.0|^6.0"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "AxlMedia\\OddstakeSdk\\OddstakeSdkServiceProvider"
            ]
        }
    }
}
